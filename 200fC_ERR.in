
Eo = 0 ; 			# Energy [eV]
G=1-qe*Eo/(me*c*c) ;
initrad= 12.74*1e-6;
Qtot= -0.2*1e-12;
temit= 11.*1e-12;
numpart=65536.;
accuracy(6) ;

setparticles("beam",numpart,me,qe,Qtot);
setrxydist("beam","g",0,initrad,0,3.);
setphidist("beam","u",0,2*pi);
settdist("beam","u",0,temit);

GBr = 10e-3 ;
setGBrxydist("beam","u",GBr/2,GBr) ;
setGBphidist("beam","u",0,2*pi) ;
